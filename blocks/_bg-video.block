{.section video}
  <div class="video-background content-fill" data-config-url="{url}" data-config-playback-speed="{playbackSpeed}" data-config-filter="{filter}" data-config-filter-strength="{filterStrength}">
    {.section videoFallbackContentItem}
      <img {@|image-meta} class="lazy-load custom-fallback-image" data-load="viewport" />
    {.end}

    <div id="player"></div>

  </div>
{.end}