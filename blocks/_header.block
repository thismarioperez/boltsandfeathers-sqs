<header class="header">
  {.section website}
  <h1>
    <a href="/" data-content-field="site-title">
      {.section logoImageUrl}
      <img src="{logoImageUrl}?format=original" alt="{siteTitle}" /> {.or} {siteTitle} {.end}
    </a>
  </h1>

  {.section siteTagLine}
  <p>{@}</p>
  {.end} {.end}
</header>