<!doctype html>
<html class="is-page-loading--first-instance" style="touch-action: manipulation;" {.if authenticatedAccount}data-authenticated-account{.end}>
  <head>
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta name="viewport" content="width=device-width, initial-scale=1" />
    <link rel="dns-prefetch" href="//static1.squarespace.com">
    <!-- critical stylesheet -->
    <link rel="stylesheet" href="/assets/styles/app.css?v={squarespace.template-revision}">
    {squarespace-headers}
  </head>
  <body id="{squarespace.page-id}" class="{squarespace.page-classes}" data-controller="test">

    {@|apply app.block}

    <script type="text/javascript" src="/scripts/app.js"></script>

    {squarespace-footers}

  </body>
</html>
